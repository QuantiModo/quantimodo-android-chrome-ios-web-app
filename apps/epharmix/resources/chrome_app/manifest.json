{
  "name": "Epharmix",
  "description": "Improving Health Outcomes",
  "version": "IONIC_APP_VERSION_NUMBER",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "icons": {
      "16" : "www/img/icons/icon_16.png",
      "48" : "www/img/icons/icon_48.png",
      "128" : "www/img/icons/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["www/js/chrome/background.js"]
    }
  },
  "permissions": [
    "identity",
    "geolocation",
    "storage",
    "notifications",
    "alarms",

    "http://app.quantimo.do/*"
  ]
}
