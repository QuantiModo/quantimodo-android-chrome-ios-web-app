  "scripts": {
    "install:global:packages": "nvm install 10.24.1 && nvm use 10.24.1 && npm install -g gulp bower ionic",
    "serve": "ionic serve src",
    "cy:open:development": "cypress install && cypress open --config-file cypress/config/cypress.development.json",
    "cy:open:localhost": "cypress install && cypress open --config-file cypress/config/cypress.localhost.json",
    "cy:open:ionic": "cypress open --config-file cypress/config/cypress.ionic.json",
    "bower": "bower install --allow-root --quiet",
    "build": "bower install --allow-root --quiet && gulp configureAppAfterNpmInstall && npm run types",
    "ci": "npm run test",
    "circle": "npm run lint && npm run test:ci && make test && npm run pre-publish",
    "compile": "npm run types",
    "configure:app": "npm run types:ts && npm run bower && gulp configureAppAfterNpmInstall",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
    "cy:chrome": "cypress run --browser chrome",
    "cy:heroku-test-setup": "whoami && node -e 'console.log(process.geteuid())' && npx cypress install && npx cypress cache path && npx cypress cache list && npx cypress verify",
    "cy:open": "cypress open --config-file cypress.json",
    "cy:open:production": "cypress open --config-file cypress/config/cypress.production.json",
    "cy:open:staging": "cypress install && cypress open --config-file cypress/config/cypress.staging.json",
    "cy:record": "cypress run --record",
    "cy:record:measurements:ionic": "cypress run --record --key 64dd30de-4365-4699-85fe-e3be5e95da65 --config-file cypress/config/cypress.ionic.json --spec cypress/integration/ionic_measurements_spec.js",
    "cy:record:measurements:production": "cypress run --record --key 64dd30de-4365-4699-85fe-e3be5e95da65 --config-file cypress/config/cypress.production.json --spec cypress/integration/ionic_measurements_spec.js",
    "cy:record:parallel": "cypress run --record --parallel",
    "cy:record:production": "cypress run --record --key 64dd30de-4365-4699-85fe-e3be5e95da65 --config-file cypress/config/cypress.production.json",
    "cy:record:staging": "cypress run --record --key 64dd30de-4365-4699-85fe-e3be5e95da65 --config-file cypress/config/cypress.staging.json",
    "cy:report:delete": "shx rm cypress/reports/*.json",
    "cy:report:mochawesome:generate": "npx mochawesome-report-generator cypress/reports/mocha/mochawesome.json --charts true --showPassed true --autoOpen true --cdn true --reportDir cypress/reports/mocha",
    "cy:report:mochawesome:merge": "npx mochawesome-merge --reportDir cypress/reports/mocha > cypress/reports/mocha/mochawesome.json",
    "cy:report:slack": "npx ts-node cypress/reports/cypress-slack-reporter.ts",
    "cy:run": "npx ts-node cypress/cypress-runner.ts",
    "cy:run:development": "cypress run --browser chrome --config-file cypress/config/cypress.development.json && npm run report:slack",
    "cy:run:localhost": "cypress run --browser chrome --config-file cypress/config/cypress.localhost.json && npm run report:slack",
    "cy:run:docs": "cypress run --spec cypress/integration/docs_spec.js",
    "cy:run:onboarding:ionic": "cypress run --config-file cypress/config/cypress.ionic.json --spec cypress/integration/ionic_onboarding_spec.js",
    "cy:run:measurements:ionic": "cypress run --config-file cypress/config/cypress.ionic.json --spec cypress/integration/ionic_measurements_spec.js",
    "cy:run:production": "cypress run --browser chrome --config-file cypress/config/cypress.production.json && npm run report:slack",
    "cy:run:staging": "cypress run --browser chrome --config-file cypress/config/cypress.staging.json && npm run report:slack",
    "cy:run:wordpress": "cypress run --spec cypress/integration/wordpress_spec.js",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "deploy:prepare": "./scripts/create_npmrc_file.sh",
    "effective:config": "circleci config process circle.yml | sed /^#/d",
    "git:pre:commit": "npm run lint:fix && npm run types && npm run test:mocha",
    "git:delete-feature-branches": "git branch | grep \"/feature\" | xargs git branch -D",
    "heroku-deploy": "git push git@heroku.com:medimodo.git HEAD:master -f",
    "heroku-postbuild": "npm run configure:app",
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-push": "git push git@heroku.com:medimodo.git HEAD:master -f",
    "heroku-test": "whoami && node -e 'console.log(process.geteuid())' && npm run ci:set-port && npm run test:ci:record",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:yaml",
    "lint:es": "eslint .",
    "lint:es-fix": "eslint . --fix",
    "lint:fix": "npm run lint:es-fix && npm run lint:ts-fix",
    "lint:ts": "cd ts && tslint --project .",
    "lint:ts-fix": "cd ts && tslint --project . --fix",
    "lint:yaml": "yamllint '*.yml' 'basic/*.yml' .buildkite/pipeline.yml .semaphore/semaphore.yml '.github/workflows/*.yml'",
    "preinstall": "echo \"STARTING npm install\"",
    "postinstall": "echo \"DONE WITH npm install\"",
    "print-env": "print-env",
    "pull-develop": "bash scripts/git-pull-develop.sh",
    "release": "npx semantic-release",
    "report": "npm run report:mochawesome && npm run report:slack",
    "report:mochawesome": "npm run mochawesome:merge && npm run mochawesome:generate",
    "semantic-release": "semantic-release",
    "ship-it": "npm run pre-publish && npm publish",
    "start": "node app.js",
    "start:ci": "serve --no-clipboard --listen ${PORT:-8080}",
    "test": "npm run test:mocha && npm run cy:run && npm run test:ghostinspector",
    "test:ghostinspector": "npx ts-node ts/gi-runner.ts",
    "test:mocha": "mocha tests/mocha/**/*.test.js",
    "test:mocha:ts": "mocha -r ts-node/register tests/mocha/**/*.test.ts",
    "types": "npm run types:cypress && npm run types:ts",
    "types:cypress": "cd cypress && tsc",
    "types:ts": "cd ts && tsc",
    "up": "docker-compose up --abort-on-container-exit --exit-code-from e2e",
    "watch": "tsc --watch --noEmit --project ./ts/tsconfig.json",
    "openapi": "npx openapi-typescript-codegen --input ./docs/open-api-v3.yaml --output ./src/api"
  },
