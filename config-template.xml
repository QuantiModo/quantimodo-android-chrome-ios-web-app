<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="APP_IDENTIFIER_PLACEHOLDER" version="2.2.2" ios-CFBundleVersion="2.2.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>APP_DISPLAY_NAME_PLACEHOLDER</name>
  <description>
